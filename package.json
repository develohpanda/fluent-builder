{
  "name": "fluent-builder",
  "version": "1.0.0",
  "description": "A typed, fluent builder for creating objects in Typescript",
  "repository": "https://github.com/develohpanda/fluent-builder",
  "author": "Opender Singh <opender94@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:cover": "jest --coverage",
    "start": "start-storybook -p 6006",
    "compile": "tsc",
    "lint": "eslint {.storybook,stories,src,__tests__}/**/* --ext .{{t,j}s{,x}}",
    "lint:fix": "yarn lint --fix",
    "local-pack": "rimraf ./*.tgz && yarn lint && yarn compile && yarn pack",
    "now-build": "build-storybook -o dist"
  },
  "lint-staged": {
    "*.{{t,j}s{,x}}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-simple-import-sort": "4.0.0",
    "husky": "3.0.8",
    "jest": "24.9.0",
    "lint-staged": "8.2.1",
    "lodash": "4.17.15",
    "prettier": "1.18.2",
    "prop-types": "15.7.2",
    "rimraf": "3.0.0",
    "ts-jest": "24.1.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "lodash": ">=4"
  },
  "dependencies": {}
}
