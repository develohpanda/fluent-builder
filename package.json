{
  "name": "@develohpanda/fluent-builder",
  "version": "0.1.1",
  "description": "A typed, fluent builder for creating objects in Typescript",
  "repository": "https://github.com/develohpanda/fluent-builder",
  "author": "Opender Singh <opender94@gmail.com>",
  "license": "MIT",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "scripts": {
    "test": "jest",
    "test:cover": "jest --coverage",
    "test:watch": "jest --watch",
    "compile": "tsc",
    "lint": "eslint {src,test}/**/* --ext .{{t,j}s{,x}}",
    "lint:fix": "yarn lint --fix",
    "local-pack": "rimraf ./*.tgz && yarn lint && yarn compile && yarn pack"
  },
  "lint-staged": {
    "*.{{t,j}s{,x}}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/prop-types": "^15.7.3",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-simple-import-sort": "4.0.0",
    "husky": "3.0.8",
    "jest": "24.9.0",
    "lint-staged": "8.2.1",
    "lodash.clonedeep": "^4.5.0",
    "prettier": "1.18.2",
    "prop-types": "15.7.2",
    "rimraf": "3.0.0",
    "ts-jest": "24.1.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "lodash.clonedeep": ">=4.5.0",
    "prop-types": ">=15.7.2"
  }
}